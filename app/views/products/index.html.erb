<h1>Products#index</h1>
<p>Find me in app/views/products/index.html.erb</p>
<p>
    <%= link_to "Create New Product", new_product_path %>
</p>
<%= form_tag products_path, method: :put do %>
<table>
    <tr><th></th>
    <th>Name</th>
    <th>Category</th></tr>
    
<% @products.each do |product| %>
<tr><td><%=check_box_tag("product.name[]", product.name, product.id)%></td>
<td><%=label_tag(:product, product.name)%></td>
<td><%=label_tag(:product, product.category)%></td>
<td><%= link_to "Edit", edit_product_path(product) %></td>
<td><%= link_to "Show", product_path(product) %></td>
<td><%= link_to "Delete", product_path(product), method: :delete %></td></tr>



<% end %>
</table>
<%= submit_tag "check Product" %>

<% end %>

